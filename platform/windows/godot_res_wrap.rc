#include "core/version.h"

#ifndef RT_MANIFEST
#define RT_MANIFEST 24
#endif

GODOT_ICON ICON platform/windows/godot_console.ico
1 RT_MANIFEST "godot.manifest"

1 VERSIONINFO
FILEVERSION     BLAZIUM_VERSION_MAJOR,BLAZIUM_VERSION_MINOR,BLAZIUM_VERSION_PATCH,0
PRODUCTVERSION  BLAZIUM_VERSION_MAJOR,BLAZIUM_VERSION_MINOR,BLAZIUM_VERSION_PATCH,0
FILEOS          4
FILETYPE        1
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName",            "Blazium Engine"
            VALUE "FileDescription",        GODOT_VERSION_NAME " (Console)"
            VALUE "FileVersion",            BLAZIUM_VERSION_NUMBER
            VALUE "ProductName",            GODOT_VERSION_NAME " (Console)"
            VALUE "Licence",                "MIT"
            VALUE "LegalCopyright",         "(c) 2024-present Blazium Engine contributors, (c) 2007-present Juan Linietsky, Ariel Manzur and Godot Engine contributors"
            VALUE "Info",                   "https://blazium.app"
            VALUE "ProductVersion",         BLAZIUM_VERSION_FULL_BUILD
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
END
